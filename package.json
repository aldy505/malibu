{
  "name": "malibu",
  "version": "1.0.0",
  "description": "Framework agnostic CSRF middleware",
  "keywords": [
    "csrf",
    "middleware",
    "framework agnostic"
  ],
  "author": "Reinaldy Rafli <aldy505@tutanota.com>",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json",
    "./": "./"
  },
  "files": [
    "dist/index.js",
    "dist/index.d.ts",
    "dist/token.d.ts",
    "dist/utils.d.ts",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "coverage": "c8 --reporter=lcov --reporter=text pnpm test",
    "test": "node --experimental-loader esbuild-node-loader node_modules/uvu/bin.js test",
    "pretest": "pnpm format:fix",
    "build": "rollup -c rollup.config.js",
    "lint": "eslint . --ext=js,mjs,ts,cjs",
    "format": "prettier --check \"./**/*.{ts,js,mjs,cjs}\"",
    "format:fix": "prettier --write \"./**/*.{ts,js,mjs,cjs}\"",
    "dev:install": "pnpm install && husky install",
    "prerelease": "pnpm lint && pnpm build && pnpm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tinyhttp/malibu"
  },
  "bugs": {
    "url": "https://github.com/tinyhttp/malibu/issues"
  },
  "homepage": "https://github.com/tinyhttp/malibu#readme",
  "directories": {
    "test": "./test",
    "lib": "./src",
    "example": "./examples"
  },
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "12.1.4",
    "@commitlint/config-conventional": "12.1.4",
    "@rollup/plugin-typescript": "8.2.1",
    "@tinyhttp/app": "1.3.11",
    "@tinyhttp/cookie-parser": "1.3.11",
    "@types/cookie-parser": "1.4.2",
    "@types/express": "4.17.12",
    "@types/node": "15.12.4",
    "@types/polka": "^0.5.2",
    "@typescript-eslint/eslint-plugin": "4.28.0",
    "@typescript-eslint/parser": "4.28.0",
    "c8": "7.7.3",
    "cookie-parser": "1.4.5",
    "esbuild-node-loader": "^0.1.1",
    "eslint": "7.29.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "3.4.0",
    "express": "4.17.1",
    "husky": "6.0.0",
    "milliparsec": "2.0.5",
    "polka": "^0.5.2",
    "prettier": "2.3.1",
    "rollup": "2.52.2",
    "supertest-fetch": "1.4.3",
    "tslib": "2.3.0",
    "typescript": "4.3.4",
    "uvu": "0.5.1"
  },
  "dependencies": {
    "@tinyhttp/cookie": "1.3.0",
    "@tinyhttp/cookie-signature": "1.3.0"
  }
}
